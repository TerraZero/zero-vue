{
  "includes": [
    "zero-system",
    "zero-scaffold"
  ],
  "scaffold": {
    "modules": [
      "zero-system",
      "zero-scaffold"
    ],
    "files": [
      {
        "type": "nuxt-loader",
        "namespace": "nuxt-loader",
        "pattern": "*.loader.js"
      },
      {
        "type": "system",
        "namespace": "scaffold",
        "pattern": "**/*.js"
      },
      {
        "type": "prisma",
        "namespace": "prisma",
        "pattern": "*.prisma"
      },
      {
        "type": "model",
        "namespace": "model",
        "pattern": "**/*.prisma"
      },
      {
        "type": "zero-config",
        "namespace": "zero-config",
        "pattern": "*.zero.json"
      }
    ]
  }
}