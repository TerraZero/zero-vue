{
  "scaffold": {
    "modules": [
      "zero-system"
    ],
    "files": [
      {
        "type": "nuxt-loader",
        "namespace": "nuxt-loader",
        "pattern": "*.loader.js"
      },
      {
        "type": "system",
        "namespace": "scaffold",
        "pattern": "**/*.js"
      },
      {
        "type": "prisma",
        "namespace": "prisma",
        "pattern": "*.prisma"
      },
      {
        "type": "model",
        "namespace": "model",
        "pattern": "**/*.prisma"
      }
    ]
  }
}